gitignore
# =========================================
# Folders and files generated by Android Studio and Gradle
# =========================================
*.iml
.gradle/
/local.properties
/.idea/
.DS_Store
/build/
/captures/
.cxx/
*.apk
*.aab
*.ap_
*.apks
*.dex
*.so

# =========================================
# Private signing keys (VERY IMPORTANT)
# =========================================
# Do NOT commit your signing keys to a public repository!
*.keystore
*.jks
*.private_key.pepk

# =========================================
# Local native build outputs
# =========================================
/app/build/
/app/libs/
/app/src/main/jniLibs/

# =========================================
# Log files
# =========================================
*.log

# =========================================
# Other IDE-specific files
# =========================================
.idea/workspace.xml
.idea/libraries/
.idea/navEditor.xml
.idea/assetWizardSettings.xml
